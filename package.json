{
  "name": "simple-react-app",
  "version": "1.0.0",
  "description": "Simple React app - with ES6+ babel transpilation, ESLint linting, Hot Module Reloading, and Webpack bundled, and Jest test framework enabled.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build:dev": "rimraf dist && webpack --mode development --config webpack.dev.config.js",
    "build:prod": "rimraf dist && webpack --mode production --config webpack.prod.config.js",
    "server": "babel-node ./src/server/server-dev.js",
    "start": "npm-run-all server --env.mode=development"
  },
  "repository": "https://github.com/aedilyan/simple-react-app",
  "keywords": [
    "simple react app with webpack",
    "react with webpack",
    "react with Express"
  ],
  "author": "Artur Edilyan",
  "license": "MIT",
  "dependencies": {
    "acorn": "^7.1.0",
    "axios": "^0.19.0",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.1.2"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "open": "^6.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "^16.10.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.css|less$": "identity-obj-proxy",
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
